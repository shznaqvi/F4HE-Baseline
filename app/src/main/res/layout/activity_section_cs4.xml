                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:enabled="false"

                                android:checked="@{child.cs4q01.equals(`2`)}"
                                android:onClick="@{()->child.setCs4q01(`2`)}"
                                android:text="@string/cs4q0102" />

                        </RadioGroup>

                    </LinearLayout>

